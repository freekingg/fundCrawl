{
  "manifest_version": 2,
  "name": "uufirst",
  "description": "My first chrome extension.",
  "version": "1.0.0",
  "icons": {
    "16": "logo192.png",
    "128": "logo192.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "logo192.png",
      "38": "logo192.png"
    },
    "default_title": "uufirst",
    "default_popup": "./index.html"
  },
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": [
      "./chrome/background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "./index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}